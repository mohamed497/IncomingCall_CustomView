<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="IncomingCallView">
        <attr name="x_position" format="float" />
        <attr name="y_position" format="float" />
        <attr name="callStatus" format="enum">
            <enum name="ringing" value="0" />
            <enum name="accept" value="1" />
            <enum name="reject" value="2" />
            <enum name="mute" value="3" />
            <enum name="msg" value="4" />
        </attr>
    </declare-styleable>
</resources>